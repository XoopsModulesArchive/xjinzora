		function _constructor($arg = array())
		function getDateAdded()
		function getName()
		function getLevel()
		function getPath($type = "array")
		function getPlayCount()
		function increasePlayCount()
		function setPlayCount()
		function getMainArt()
		function addMainArt($image)
		function getRandomArt()
		function addRandomArt($image)
		function getShortDescripiton()
		function addShortDescription($text)
		function getDescription()
		function addDescription($text)
		function getRating()
		function addRating($rating)
		function getDiscussion()
		function addDiscussion($text,$username)
		function getYear()
		function isLeaf()
	class jzMediaNodeClass extends jzMediaElement
		function jzMediaNodeClass($par = array())
		function setNaturalDepth($n) // changes the 'natural' search/count depth
		function getNaturalDepth() // pretty much only used internally.
		function getSubNodeCount($type='both', $distance=1)
		function getSubNodes($type='nodes',$distance=1, $random=false, $limit=0)
		function getSubNodesRecursive($type='leaves',$random=false, $limit=0)
		function getTopSubNodes($type='nodes',$top_type='most-played', $distance=1, $limit=0)
		function getTopSubNodesRecursive($type='leaves',$top_type='most-played',$limit=0)
		function getSubNode($name)
		function search($searchArray, $op = "and", $type='both', $depth = -1, $meta = array(), $exclude = array(), $limit=0)
		function updateCache()
		function isLeaf() { return false; }
		function getAlphabetical($letter, $depth = 1)
	class jzMediaTrackClass extends jzMediaElement
		function jzMediaTrackClass($par = array())
		function getFileName()
		function getMeta($mode = "cache")
		function getLyrics()
		function addLyrics($text)
